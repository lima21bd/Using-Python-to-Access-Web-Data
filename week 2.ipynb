    "import re\n",
    "\n",
    "file_open = open('sample.txt')\n",
    "emp_list = []\n",
    "tot = 0\n",
    "for line in file_open:\n",
    "    \n",
    "    s = re.findall('[0-9]+', line)\n",
    "    \n",
    "    emp_list = emp_list + s\n",
    "    \n",
    "for x in emp_list:\n",
    "        \n",
    "    convTodigit = int(x)\n",
    "        \n",
    "    tot =  tot + convTodigit\n",
    "print(tot)"
   ]
